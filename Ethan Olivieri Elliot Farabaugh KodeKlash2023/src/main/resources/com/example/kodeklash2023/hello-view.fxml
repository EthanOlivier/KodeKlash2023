<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.kodeklash2023.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <AnchorPane prefHeight="600.0" prefWidth="800.0">
         <children>
            <Label fx:id="lblUsername" layoutX="333.0" layoutY="167.0" text="Username displayed here" />
            <Button fx:id="btnConnect" layoutX="370.0" layoutY="300.0" mnemonicParsing="false" onAction="#onbtnConnectClick" text="Connect" />
         </children>
      </AnchorPane>
   </children>
</VBox>
